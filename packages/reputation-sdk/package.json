{
  "name": "@truthbounty/reputation-sdk",
  "version": "0.1.0",
  "description": "Decentralized reputation SDK for prediction markets - index any protocol, unified TruthScore",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters/pancakeswap": {
      "import": "./dist/adapters/pancakeswap.js",
      "types": "./dist/adapters/pancakeswap.d.ts"
    },
    "./adapters/polymarket": {
      "import": "./dist/adapters/polymarket.js",
      "types": "./dist/adapters/polymarket.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test",
    "example": "node examples/basic-usage.js"
  },
  "dependencies": {
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "prediction-markets",
    "reputation",
    "blockchain",
    "sdk",
    "pancakeswap",
    "polymarket",
    "defi"
  ],
  "author": "TruthBounty",
  "license": "MIT"
}
